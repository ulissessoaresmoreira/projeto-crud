<!DOCTYPE html>
<html lang="pt-br">
<head>
    
    <%- include('partials/head.ejs')%>
    
</head>
<body>
    <h1> Listagem de Usu√°rios </h1>
    <%- include('partials/navbar.ejs') %>
    
    <div class="list">
        <% users.forEach((user) => {%>
            <li>
                <%= user.name%> - <%= user.email%> - Idade:<%= user.age%>
            </li>
        <% }) %>
    </div>
    <hr>


    <div class="tabela">
        <table border='1' class="tabela">
            <tr>
                <td>Nome</td>
                <td>E-mail</td>
                <td>Idade</td>
            </tr>
            <tr>
                <% users.forEach((user) => {%>
                <td><%= user.name%></td>
                <td><%= user.email%></td>
                <td><%= user.age%></td>
                <% }) %>
            </tr>

        </table>
    </div>


    <table border=1 bgcolor="green" width=800 height=100>
        <tr height=30>

        <td>
        Nome
        </td>

        <td>
        E-mail
        </td>

        <td>
        Idade
        </td>

        </tr>


        <tr height=70>
            <% users.forEach((user) => {%>
        <td>
            <%= user.name%>
        </td>
        
        <td>
            <%= user.email%>
        </td>

        <td>
            <%= user.age%> 
            </td>
            <% }) %>
        </tr>
        

        </table>
        

    

    
    <%- include('partials/foot.ejs')%>
</body>
</html>